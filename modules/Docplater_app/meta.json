{
	"package"          : "Docplater_app",
	"category"         : "modules",
	"version"          : "0.45.0+build-60",
	"description"      : "Main Docplater UI",
	"author"           : "Nazar Mokrynskyi",
	"license"          : "AGPL-3.0",
	"assets"           : {
		"Docplater_app" : "*"
	},
	"db"               : [
		"docplater"
	],
	"db_support"       : [
		"MySQLi"
	],
	"provide"          : "docplater_app",
