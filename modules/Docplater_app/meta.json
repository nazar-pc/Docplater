		"scribe-plugin-inline-styles-to-elements" : "^0.1",
		"scribe-plugin-keyboard-shortcuts"        : "^0.1",
		"scribe-plugin-sanitizer"                 : "^0.1",
		"scribe-plugin-tab-indent"                : "^0.1",
		"scribe-plugin-toolbar"                   : "^1.0.0",
		"lodash-amd"                              : "3.5.0",
		"seamless-immutable"                      : "^6.3",
		"polymer-redux"                           : "^0.3",
		"redux"                                   : "^3.6"
	}
